#!/bin/sh
URL=https://www.serienjunkies.de/docs/serienkalender-aktuell.html
curl -s $URL | grep -Po '(?<=<div class="sjf-show-calendar-date"> ).+?(?= </div>)|<div class="sjf-show-calendar-title"> <a href=[^>]+>\K.+?(?= </a>)' |
grep -E --color=always '[DFMS][aceghimnorstw]{5,10}, [0-9]{1,2}. [ADFJMNOS][a√§begiklmnoprstuvz]{2,8} 202[0-9]' -z | column
echo ; echo $URL
